<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Controller.Worker.WorkerCancelWindowController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" opacity="0.52" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../wrightelectric.jpg" />
         </image>
      </ImageView>
      <Button fx:id="backButton" layoutX="27.0" layoutY="337.0" mnemonicParsing="false" onAction="#back" text="Cofnij" />
      <Label fx:id="title" layoutX="270.0" layoutY="51.0" text="Usuń lot">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="clockLabel" layoutX="520.0" layoutY="23.0" text="00:00:00" />
      <Label fx:id="departure" layoutX="92.0" layoutY="102.0" text="Miasto wylotu">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="arrival" layoutX="87.0" layoutY="183.0" text="Miasto przylotu">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="date" layoutX="112.0" layoutY="260.0" text="Termin">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <ComboBox fx:id="departureComboBox" layoutX="224.0" layoutY="98.0" onAction="#departure" prefWidth="150.0" />
      <ComboBox fx:id="arrivalComboBox" layoutX="221.0" layoutY="179.0" onAction="#arrival" prefWidth="150.0" />
      <ComboBox fx:id="dateComboBox" layoutX="221.0" layoutY="256.0" onMouseClicked="#date" prefWidth="150.0" />
      <Button fx:id="addButton" layoutX="277.0" layoutY="295.0" mnemonicParsing="false" onAction="#add" text="Usuń" />
      <Label fx:id="infoLabel" alignment="CENTER" layoutX="177.0" layoutY="340.0" prefHeight="18.0" prefWidth="238.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
